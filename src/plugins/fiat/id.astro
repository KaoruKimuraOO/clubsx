---
import type { Currency } from '@crossmint/client-sdk-base'
import type { Product } from '@constants/products'
import IdPage from './Id'

const {
  cm,
  paymentCurrency,
  product,
} = Astro.props as {
  cm: {
    projectId: string,
    collectionId: string,
    environment?: string,
  }
  paymentCurrency?:Currency,
  product: Product
}
---


<IdPage client:load cm={cm} paymentCurrency={paymentCurrency} product={product} />

