---
import JoinCheckout from '@components/Join/JoinCheckout.vue'

const {
  price,
  propertyAddress,
  currency,
  rpcUrl,
  feeBeneficiary,
  feePercentage,
  payload,
} = Astro.props
---

<JoinCheckout
  client:only="vue"
  amount={price}
  destination={propertyAddress}
  currency={currency}
  feeBeneficiary={feeBeneficiary}
  feePercentage={feePercentage}
  page="JOIN"
  rpcUrl={rpcUrl}
  payload={payload}
/>
